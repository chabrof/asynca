  "scripts": {
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "npm run build:dev:browser && npm run build:dev:node",
    "build:dev:browser": "cd \"pack_browser/dev\" && pwd && \"../../node_modules/.bin/webpack\" --mode development",
    "build:browser:dev": "npm run build:dev:browser",
    "build:dev:node": "cd \"pack_node/dev\" && pwd && \"../../node_modules/.bin/webpack\" --mode development",
    "build:node:dev": "npm run build:dev:node",
    "build:prod": "npm run build:prod:browser && npm run build:prod:node",
    "build:prod:browser": "cd \"pack_browser/prod\" && pwd && \"../../node_modules/.bin/webpack\" --mode production",
    "build:browser:prod": "npm run build:prod:browser",
    "build:prod:node": "cd \"pack_node/prod\" && pwd && \"../../node_modules/.bin/webpack\" --mode production",
    "build:node:prod": "npm run build:prod:node",
    "build:samples": "sh \"./node_modules/.bin/webpack\" --config \"pack_browser/samples/webpack.config.js\"",
    "test": "mocha-webpack -r \"jsdom-global/register\" --colors --full-trace --webpack-config \"./pack_node/prod/webpack.config.js\" \"test/**/*.js\""
  },
